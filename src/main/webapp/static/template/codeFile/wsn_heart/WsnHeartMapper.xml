<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ai.mnt.persistence.sensor.WsnHeartMapper">

    <resultMap id="WsnHeartResultMap" type="com.ai.mnt.model.sensor.WsnHeart">
        <result column="ware_id" property="wareId" />
        <result column="obj_id" property="objId" />
        <result column="heart_value" property="heartValue" />
        <result column="rate" property="rate" />
        <result column="state" property="state" />
        <result column="create_time" property="createTime" />
        <result column="field" property="field" />
        <id column="id" property="id" />
    </resultMap>

    <sql id="WsnHeartColumns">
        ware_id,
        obj_id,
        heart_value,
        rate,
        state,
        create_time,
        field,
        id
    </sql>

    <insert id="save" useGeneratedKeys="true" keyProperty="id">
        insert into wsn_heart (
        ware_id,
        obj_id,
        heart_value,
        rate,
        state,
        create_time,
        field,
        )
        values (
        #{wareId},
        #{objId},
        #{heartValue},
        #{rate},
        #{state},
        #{createTime},
        #{field},
        )
    </insert>

    <insert id="saveAll">
        insert into wsn_heart (
        ware_id,
        obj_id,
        heart_value,
        rate,
        state,
        create_time,
        field,
        )
        values
        <foreach item="item" collection="list" separator=",">
            (
            #{item.wareId},
            #{item.objId},
            #{item.heartValue},
            #{item.rate},
            #{item.state},
            #{item.createTime},
            #{item.field},
            )
        </foreach>
    </insert>

    <delete id="deleteByPrimaryKey">
        delete from wsn_heart
        where
        id = #{id}
    </delete>

    <delete id="deleteList">
        delete from wsn_heart
        where id in
        <foreach item="item" collection="list" separator="," open="(" close=")">
            #{item}
        </foreach>
    </delete>

    <update id="updateByPrimaryKey">
        update wsn_heart
        <set>
            <if test="wareId != null">
                ware_id = #{wareId},
            </if>
            <if test="objId != null">
                obj_id = #{objId},
            </if>
            <if test="heartValue != null">
                heart_value = #{heartValue},
            </if>
            <if test="rate != null">
                rate = #{rate},
            </if>
            <if test="state != null">
                state = #{state},
            </if>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="field != null">
                field = #{field},
            </if>
        </set>
        where
        id = #{id}
    </update>

    <select id="findByPrimaryKey" resultMap="WsnHeartResultMap">
        select
        <include refid="WsnHeartColumns" />
        from wsn_heart
        where id = #{id}
    </select>

    <select id="findList" resultMap="WsnHeartResultMap">
        select
        <include refid="WsnHeartColumns" />
        from wsn_heart
        <where>
            <if test="wareId != null and wareId != ''">
                ware_id = #{wareId}
            </if>
            <if test="objId != null and objId != ''">
                and obj_id = #{objId}
            </if>
            <if test="heartValue != null and heartValue != ''">
                and heart_value = #{heartValue}
            </if>
            <if test="rate != null and rate != ''">
                and rate = #{rate}
            </if>
            <if test="state != null">
                and state = #{state}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime}
            </if>
            <if test="field != null and field != ''">
                and field = #{field}
            </if>
            <if test="id != null">
                and id = #{id}
            </if>
        </where>
    </select>

    <select id="findAll" resultMap="WsnHeartResultMap">
        select
        <include refid="WsnHeartColumns" />
        from wsn_heart
    </select>
    
    <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>
    
</mapper>