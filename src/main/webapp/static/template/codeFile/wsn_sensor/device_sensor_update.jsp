                                        <label class="col-md-4 control-label" for="form_control_1">
                                            噪声传感器ID <span class="required">*</span>
                                        </label>
                                        <div class="col-md-8">
                                            <input type="text" class="form-control" placeholder="" value="${wsnSensor.noiseId}" name="noiseId" id="noiseId">
