<%@include file="../../included/includedJs.jsp" %>

<script type="text/javascript">

jQuery(document).ready(function() {
    
    //表单校验提交
    //[1]自定义校验规则
    var rules = {
            id:"required",
            temperatureId:"required",
            humidityId:"required",
            dustId:"required",
            noiseId:"required",
            temperature:"required",
            humidity:"required",
            dust:"required",
            noise:"required",
            createTime:"required",
            rate:"required"
    };
    //[2]表单校验初始化
    initFormValidate('form-update', rules, 'device/sensor/update', 'update');
    
});

